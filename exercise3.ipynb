    "#program to perform svd\n",
    "import numpy as nbr\n",
    "from scipy.linalg import svd\n",
    "b=nbr.array([[4,5,6],[5,7,8],[7,8,9]])\n",
    "v,s,t=svd(b)\n",
    "print(v)\n",
    "print(s)\n",
    "print(t)\n",
    "e=(v @ nbr.diag(s) @t)\n",
    "print(e)"
   ]
